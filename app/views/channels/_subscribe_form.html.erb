<div class="col-lg-12 pull-right" id="sub-form<%= @channel.id %>">
  <% if current_user.subscribing?(@channel) %>
    <%= render 'channels/unsubscribe', :user => current_user %>
  <% else %>
    <%= render 'channels/subscribe', :user => current_user %>
  <% end %>
  <ul class="col-lg-3 blog-detail list-inline pull-right" style="margin-right: 10px; padding: 0;">
    <li><i class="fa fa-user"></i><%= @channel.getSubCount %></li>
  </ul>
</div>