<div class="row col-lg-12 col-md-12 col-sm-12 col-xs-12" data-aos="fade-right">
  <div class="blog-column">
    <span><%= feed.title %></span>
    <ul class="blog-detail list-inline">
      <li><i class="fa fa-user"></i><%= feed.channel.name %></li>
      <li><i class="fa fa-clock-o"></i><%= convertDate feed.created_at %></li>
    </ul>
    <p><%= feed.summary %></p>
    <a href="<%= feed.url %>">Read More</a>
  </div>
  <hr class="col-lg-12" style="border-top: 1px solid #eeeeee"/>
</div>